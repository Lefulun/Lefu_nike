    <!-- Для создания github проекта -->
    git init
    git config user.name lefu
    git config user.email hyoru@mail.ru
    git config gue.encoding utf-8

    <!-- для включения  сохранения следующий пункт -->
    git add index.html

    <!-- Для сохранения -->
    git commit -m 'first' 

    <!-- Что бы посмотретьь все изменения пишем -->

    gitk




<!--  показывает сколько веток -->
    git branch 

    

    <!--  ветка это как просто комната где редактирую проект до объединения в один . Так можно работать над одним проектом
    многими людьми типо ктото хедер кто то меин и так далее -->

    <!--  что бы переходить с ветки на ветку нужно сохранить после переходить на следующий этап -->

    <!--  git checkout .. что бы перейти по ветке то необходимо после этой команды прописать название ветки   -->
    
    
    git chechout  -b 'html'

    <!--  для быстрого создания и перехода на новую ветку -->


    <!-- для передачи сохранений на другую ветку -->

    <!-- добавили на сохранение все папки  для ветки  -->


    git add .

    <!-- после git commit  -->


    git merge 'master'

    <!-- для передачи сохранений  с выбранной ветки тут  что бы  все перенеслось в ветку где мы находимся   -->


    <!--   есть еще и удаления  -->

    git soft git mixed git hard
    <!-- подробно на видео  -->

    <!-- Команда для удаления  веток  -->

    git branch -d 'dadas'



    



